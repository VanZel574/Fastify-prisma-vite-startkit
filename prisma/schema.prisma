generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../db/Meas-db.sqlite3"
}

model Meas {
  id                    Int       @id @default(autoincrement())
  product_name          String?
  report_name           String
  report_text           String
  report_signature      String?
  report_obj            String?
  report_postanalysis   String?
  postanalysis_obj      String?
  obx                   String?
  map_6190_init         String?
  map_6190_postanalysis String?
  control_program       String?
  signature             String?
  computer_name         String?
  date                  DateTime? @default(now())
}
